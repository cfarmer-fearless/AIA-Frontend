[run]
branch = True
include=
    *.py
omit=
    */__init__.py
    */python3.9/*
    */tests/*
    */.venv/*
    */lang/*

[report]
fail_under = 90
exclude_lines=
    # Have to re-enable the standard pragma
    pragma: no cover

[html]
directory = html_coverage_reports
title = Coverage Report for Fearless Proposal Platform